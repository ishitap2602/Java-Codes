<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-4.0.xsd">
	
	<bean id = "ord1" class="com.test.Order1">
		
		<property name = "id" value = "1" ></property>
		<property name = "name" value = "Apple"></property>
		<property name = "price" value="150"></property>
<!-- 		<property name = "salary" value="20000"></property> -->
 	</bean>
 	
	<bean id="ord2" class="com.test.Order1">
        <property name="id" value="2"/>
        <property name="name" value="Banana"/>
        <property name="price" value="75"/>
    </bean>
 	
 	
 	<bean id="addr1" class="com.test.Address">
 		<property name = "city" value = "Mumbai" ></property>
		<property name = "state" value = "Maharashtra"></property>
 	</bean>
 	
 	<bean id="addr2" class="com.test.Address">
 		<property name = "city" value = "Bangalore" ></property>
		<property name = "state" value = "Karnataka"></property> 	
 	</bean>
 	
 	<bean id="cst" class="com.test.Customer">
 		<property name = "id" value = "101" ></property>
		<property name = "name" value = "Ishita"></property>
		
		<property name = "orders">
			<list>
			<ref bean="ord1"/>
			<ref bean="ord2"/>
			</list>
		</property>
		
		<property name="addr">
		<list>
		<ref bean="addr1"/>
		<ref bean="addr2"/>
		</list>
		</property>
		
 	</bean>
 	
</beans>
 
 