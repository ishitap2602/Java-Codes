<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-4.0.xsd">
	
	<bean id = "emp" class="com.test.Employee" scope="singleton" >
		
		<property name = "id" value = "101" ></property>
		<property name = "name" value = "Ishita"></property>
		<property name = "cmp" value="Mphasis"></property>
<!-- 		<property name = "salary" value="20000"></property> -->
 	</bean>
 	
 	<bean id="emp1" class="com.test.Employee">
 		<constructor-arg name="id" value="102"></constructor-arg>
 	 	<constructor-arg name="name" value="Jay"></constructor-arg>
 	 	<constructor-arg name="cmp" value="Mphasis"></constructor-arg>
 	 	<constructor-arg name="salary" value="25000"></constructor-arg>
 	
 	</bean>
 	
</beans>
 
 